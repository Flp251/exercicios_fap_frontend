function buscaLinear ( array, elementoBuscado){ 
  for (let i = 0; i < array.length; i++){
    if(array[i] === elementoBuscado){ 
      return i;
     }
  }
return -1;
}
const listaNum = [15, 8, 10, 25, 12, 30, 5, 20, 18, 7];
const elementoProcurado = 20;

const indiceEncontrado = buscaLinear (listaNum, elementoProcurado);
if (indiceEncontrado !== -1) {
  console.log (`o elemento ${elementoProcurado} foi encontrado no indice ${indiceEncontrado}.`);
 
 } else {
    console.log("o elemento nao foi encontrado");
}
                          
